{
  "extensions": {
    "store/theme": {
      "component": "Theme"
    },
    "store/__loading": {
      "component": "Loading"
    },
    "store/__icons": {
      "component": "IconPack"
    }
  },
  "templates": {
    "store": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/StoreContextProvider",
      "props": {
        "elements": [
          "header",
          "__children__",
          "footer"
        ]
      },
      "extensions": {
        "header": {
          "component": "vtex.dreamstore-header/index"
        },
        "header/telemarketing": {
          "component": "vtex.telemarketing/index"
        },
        "header/minicart": {
          "component": "vtex.minicart/MiniCart"
        },
        "header/login": {
          "component": "vtex.login/Login"
        },
        "header/menu-link": {
          "component": "vtex.menu/Menu"
        },
        "header/category-menu": {
          "component": "vtex.category-menu/index"
        },
        "header/logo": {
          "component": "vtex.store-components/Logo"
        },
        "header/search-bar": {
          "component": "vtex.store-components/SearchBar"
        },
        "footer": {
          "component": "vtex.dreamstore-footer/index"
        }
      }
    },
    "product": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductContextProvider",
      "props": {
        "elements": [
          "details",
          "kit",
          "related"
        ]
      },
      "extensions": {
        "details": {
          "component": "vtex.product-details/ProductDetails"
        },
        "details/breadcrumb": {
          "component": "vtex.breadcrumb/Breadcrumb"
        },
        "kit": {
          "component": "vtex.product-kit/ProductKit"
        },
        "related": {
          "component": "vtex.shelf/RelatedProducts"
        }
      }
    },
    "search": {
      "component": "vtex.render-runtime/LayoutContainer",
      "context": "vtex.store@1.x/ProductSearchContextProvider",
      "props": {
        "elements": [
          "banner",
          "results"
        ]
      },
      "extensions": {
        "banner": {
          "component": "vtex.carousel/Carousel"
        },
        "results": {
          "component": "vtex.search-result/index"
        },
        "results/breadcrumb": {
          "component": "vtex.breadcrumb/Breadcrumb"
        },
        "results/gallery": {
          "component": "vtex.search-result/Gallery"
        },
        "results/gallery/product-summary": {
          "component": "vtex.product-summary/index"
        },
        "results/filter-navigator": {
          "component": "vtex.search-result/FilterNavigator"
        }
      }
    },
    "login": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "login"
        ]
      },
      "extensions": {
        "login": {
          "component": "vtex.login/LoginContent"
        }
      }
    },
    "home": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "carousel",
          "shelf"
        ]
      },
      "extensions": {
        "carousel": {
          "component": "vtex.carousel/Carousel"
        },
        "shelf": {
          "component": "vtex.shelf/Shelf"
        }
      }
    },
    "account": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "account"
        ]
      },
      "extensions": {
        "account": {
          "component": "vtex.my-account/index"
        }
      }
    },
    "simple": {
      "component": "vtex.render-runtime/LayoutContainer",
      "props": {
        "elements": [
          "container"
        ]
      }
    }
  },
  "pages": {
    "store": [
      {
        "name": "Default",
        "template": "store"
      }
    ],
    "store/home": [
      {
        "name": "Default",
        "template": "home"
      }
    ],
    "store/account": [
      {
        "name": "Default",
        "template": "account"
      }
    ],
    "store/compare": [
      {
        "name": "Default",
        "template": "simple"
      }
    ],
    "store/login": [
      {
        "name": "Default",
        "template": "login"
      }
    ],
    "store/brand": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/department": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/category": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/subcategory": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/search": [
      {
        "name": "Default",
        "template": "search"
      }
    ],
    "store/product": [
      {
        "name": "Default",
        "template": "product"
      }
    ]
  }
}
